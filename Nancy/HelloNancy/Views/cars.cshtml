@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<List<HelloNancy.Models.Car>>
@using HelloNancy.Models;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>HelloNancy</title>
    <style type="text/css">
        body
        {
            text-align: center;
        }
    </style>
</head>
<body>
    <img src="~/Content/nancy-logo.png" alt="Nancy logo" /><br />
    <div>
        This view was rendered using the Nancy Razor view engine
        @foreach (Car c in Model)
        {
            <p>        
                @c.Make
                @c.HorsePower
                @c.YearOfBuild
            </p>            
        }      
    </div>
</body>
</html>
